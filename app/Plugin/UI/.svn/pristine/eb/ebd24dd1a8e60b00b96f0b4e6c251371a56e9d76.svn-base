<?
	echo $this->Html->link(
		$this->Html->image("UI.action-cancel.png"),
		"javascript:void(0)",
		array(
			"onclick" => "undoChanges($(this).closest('.form'));",
			"escape" => false,
			'class' => 'cancel'
		)
	);
	echo $this->Html->link(
		$this->Html->image("UI.action-check.png"),
		"javascript:void(0)",
		array(
			"onclick" => "acceptChanges($(this).closest('.form'));",
			"escape" => false,
			'class' => 'check'
		)
	);